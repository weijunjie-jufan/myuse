<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
	
	<!-- namespace的值决定了当前子映射文件当中的sql都是属于哪一个接口当中的方法调用的sql -->
	
	<mapper namespace="com.wei.ssm.mapper.TestUserMapper">
		<select id="findAllUser" resultType="com.wei.ssm.pojo.TestUser">
			select * from test_user
		</select>
	
	</mapper>

